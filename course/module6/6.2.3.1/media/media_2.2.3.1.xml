<component id="ID_s3_SC1" fontFace='_typewriter' fontSize="10">	
		<feedback id="fb"><![CDATA[<b>***</b><font  face="Arial" color="#0000FF">You must enter the exact and full command.</font><b>***</b> ]]></feedback>		
		<existingcommands><![CDATA[<font  face="Arial"><b>Enable IPv6 unicast routing.</b></font>]]></existingcommands>
		<command prompt="R2(config)#">
		    <correctresponse><![CDATA[ipv6 unicast-routing]]></correctresponse>
			<instruction><![CDATA[<font face="Arial" class="syntaxBottomLine"><b>You successfully enabled IPv6 unicast routing on R2.</b>]]></instruction>
		</command>
