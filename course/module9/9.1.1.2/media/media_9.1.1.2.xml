<textcomponents>

<!-- slide 1-->
<component id="ID_s1_title">
<text><![CDATA[<b>A TCP Conversation</b>]]></text>
</component>
	
	<!--animation-->
	<component id="ID_s1_txt01">
	<text><![CDATA[TCP SYN]]></text>
	</component>
	
	<component id="ID_s1_txt02">
	<text><![CDATA[&quot;Let's talk.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt03">
	<text><![CDATA[&quot;Sure, let's talk.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt04">
	<text><![CDATA[TCP SYN/ACK]]></text>
	</component>
	
	<component id="ID_s1_txt05">
	<text><![CDATA[TCP ACK]]></text>
	</component>
	
	<component id="ID_s1_txt06">
	<text><![CDATA[&quot;Great, we have a connection.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt07">
	<text><![CDATA[TCP Data]]></text>
	</component>
	
	<component id="ID_s1_txt08">
	<text><![CDATA[&quot;I need you to send me some data.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt09">
	<text><![CDATA[&quot;Thanks, I received your request.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt10">
	<text><![CDATA[TCP ACK]]></text>
	</component>
	
	<component id="ID_s1_txt11">
	<text><![CDATA[&quot;Here is the data you want.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt12">
	<text><![CDATA[TCP Data]]></text>
	</component>
	
	<component id="ID_s1_txt13">
	<text><![CDATA[TCP ACK]]></text>
	</component>
	
	<component id="ID_s1_txt14">
	<text><![CDATA[&quot;Thanks. I received my data.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt15">
	<text><![CDATA[&quot;I am finished and have no more data to send.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt16">
	<text><![CDATA[TCP FIN]]></text>
	</component>
	
	<component id="ID_s1_txt17">
	<text><![CDATA[TCP FIN/ACK]]></text>
	</component>
	
	<component id="ID_s1_txt18">
	<text><![CDATA[&quot;I am finished as well. Thanks.&quot;]]></text>
	</component>

	<component id="ID_s1_txt19">
	<text><![CDATA[&quot;Thanks.&quot;]]></text>
	</component>
	
	<component id="ID_s1_txt20">
	<text><![CDATA[TCP ACK]]></text>
	</component>
	

